{
  "name": "font-awesome-openui5",
  "version": "3.0.2",
  "description": "Font Awesome icons for OpenUI5.",
  "main": "./dist/font-awesome-openui5.js",
  "unpkg": "./dist/autoload.js",
  "jsdelivr": "./dist/autoload.js",
  "typings": "./dist/font-awesome-openui5.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist/*",
    "build:node": "tsc -d -p .",
    "build:web": "webpack --mode production",
    "build": "run-p build:*",
    "lint": "tslint -p .",
    "pretest": "npm run build",
    "test:browser": "karma start",
    "test:node": "mocha -r ts-node/register ./test/node.test.ts",
    "test": "npm-run-all lint --parallel test:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/font-awesome-openui5.git"
  },
  "keywords": [
    "font-awesome",
    "font awesome",
    "openui5"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/font-awesome-openui5/issues"
  },
  "homepage": "https://font-awesome-openui5.surge.sh/",
  "dependencies": {
    "font-awesome-icon-chars": "^2.4.0",
    "lodash.isnil": "^4.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/lodash.isnil": "^4.0.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "8",
    "@types/openui5": "^1.40.2",
    "chai": "^4.2.0",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-json-preprocessor": "^0.3.3",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^2.0.13",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-gluons": "^2.0.3",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-stylish": "^0.1.8"
  }
}
